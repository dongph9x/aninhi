{
  "name": "eco-bot",
  "version": "1.0.0",
  "main": "src/index.ts",
  "license": "MIT",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=production DEBUG=bot:* tsx --tsconfig tsconfig.json src/index.ts",
    "dev": "cross-env NODE_ENV=development DEBUG=bot:* tsx watch --tsconfig tsconfig.json src/index.ts",
    "dev:inspect": "cross-env NODE_ENV=development DEBUG=bot:* tsx --inspect-brk --tsconfig tsconfig.json src/index.ts",
    "format:fix": "prettier --write --list-different .",
    "type-checking": "tsc --noEmit",
    "format": "prettier --check .",
    "lint-staged": "lint-staged",
    "lint:fix": "eslint . --fix",
    "lint": "eslint .",
    "prepare": "husky",
    "db:generate": "prisma generate",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:reset": "prisma migrate reset",
    "db:backup": "./scripts/backup-database.sh",
    "migrate:json": "tsx scripts/migrate.ts",
    "test:db": "tsx scripts/test-db.ts",
    "test:commands": "tsx scripts/test-commands.ts"
  },
  "dependencies": {
    "@prisma/client": "^6.12.0",
    "cross-env": "^7.0.3",
    "discord.js": "^14.19.3",
    "dotenv": "^16.5.0",
    "lodash": "^4.17.21",
    "ms": "^2.1.3",
    "prisma": "^6.12.0",
    "tsx": "^4.19.4"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.8.1",
    "@eslint/js": "^9.26.0",
    "@types/debug": "^4.1.12",
    "@types/lodash": "^4.17.16",
    "@types/ms": "^2.1.0",
    "@types/node": "^24.0.14",
    "commitlint": "^19.8.1",
    "debug": "^4.4.0",
    "eslint": "^9.26.0",
    "eslint-plugin-path-alias": "^2.1.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-unused-imports": "^4.1.4",
    "globals": "^16.1.0",
    "husky": "^9.1.7",
    "lint-staged": "^15.5.2",
    "prettier": "^3.5.3",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.32.0"
  }
}
